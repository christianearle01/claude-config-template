{
  "$schema": "claude-preferences-export-v1",
  "exportVersion": "1.0.0",
  "exportedAt": "2025-12-16T00:00:00Z",
  "exportedBy": "claude-config-template",
  "sourceVersion": "3.14.0",

  "exportType": "template",

  "templateMetadata": {
    "id": "security-first",
    "name": "Security First",
    "description": "Conservative automation with thorough security scanning - for security-conscious developers",
    "category": "security",
    "recommendedFor": ["intermediate", "advanced", "expert"],
    "version": "1.0.0",
    "author": "claude-config-template"
  },

  "compatibility": {
    "minVersion": "3.8.0",
    "maxVersion": "3.14.0"
  },

  "contents": {
    "profile": {
      "proactivityLevel": "medium",
      "preferredWorkflow": "wizard"
    },
    "confidenceThresholds": {
      "autoApply": 99,
      "suggestProminently": 85,
      "showAsOptional": 60,
      "hideBelow": 40
    },
    "skillSpecificPreferences": {
      "security-scanner": {
        "showInfoLevel": true,
        "autoFixLowRisk": false,
        "ignoredPatterns": []
      },
      "test-generator": {
        "coverageReminders": true,
        "generateMocks": true
      },
      "commit-readiness-checker": {
        "requireChangelog": true,
        "requireTests": true,
        "conventionalCommits": true
      },
      "standards-enforcer": {
        "strictMode": true,
        "autoFormat": true
      }
    },
    "learnedPreferences": {
      "quality": {
        "security-scan-threshold": "all",
        "test-coverage-target": 85,
        "lint-on-commit": true
      }
    }
  },

  "metadata": {
    "checksum": "template-security-first-v1",
    "exportOptions": {
      "includeAnalytics": false,
      "includeLearningHistory": false,
      "includeProjectPatterns": false
    }
  }
