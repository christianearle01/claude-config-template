{
  "$schema": "claude-preferences-export-v1",
  "exportVersion": "1.0.0",
  "exportedAt": "2025-12-16T00:00:00Z",
  "exportedBy": "claude-config-template",
  "sourceVersion": "4.3.0",

  "exportType": "template",

  "templateMetadata": {
    "id": "gallery-fullstack",
    "name": "Full-Stack Parameterized",
    "description": "Customizable full-stack development with parameters",
    "category": "fullstack",
    "recommendedFor": ["intermediate", "advanced"],
    "version": "1.0.0",
    "author": "claude-config-template",
    "parameters": {
      "frontendFramework": {
        "type": "string",
        "required": true,
        "description": "Frontend framework (react, vue, svelte, angular)"
      },
      "backendFramework": {
        "type": "string",
        "required": true,
        "description": "Backend framework (express, fastapi, django, rails)"
      },
      "database": {
        "type": "string",
        "default": "postgresql",
        "description": "Database (postgresql, mysql, mongodb, sqlite)"
      },
      "testCoverage": {
        "type": "number",
        "default": 80,
        "description": "Test coverage target percentage (0-100)"
      },
      "useTypeScript": {
        "type": "boolean",
        "default": true,
        "description": "Use TypeScript for type safety"
      }
    },
    "_parametersHint": "Parameters are resolved when template is applied. Required: frontendFramework, backendFramework"
  },

  "compatibility": {
    "minVersion": "4.3.0",
    "maxVersion": null
  },

  "contents": {
    "profile": {
      "experienceLevel": "intermediate",
      "proactivityLevel": "medium",
      "primaryFrameworks": ["${frontendFramework}", "${backendFramework}"]
    },

    "confidenceThresholds": {
      "autoApply": 95,
      "suggestProminently": 75,
      "showAsOptional": 50,
      "hideBelow": 30
    },

    "learnedPreferences": {
      "workflow": {
        "monorepo-friendly": true,
        "api-contracts": true
      },
      "quality": {
        "test-coverage-target": "${testCoverage}",
        "database-preference": "${database}",
        "typescript-enabled": "${useTypeScript}"
      }
    },

    "skillSpecificPreferences": {
      "test-generator": {
        "coverageTarget": "${testCoverage}",
        "e2eTests": true,
        "integrationTests": true
      },
      "security-scanner": {
        "checkFrontendXSS": true,
        "checkBackendInjection": true,
        "checkAPIAuth": true
      },
      "commit-readiness-checker": {
        "conventionalCommits": true,
        "requireTests": true
      }
    }
  },

  "metadata": {
    "checksum": "gallery-fullstack-v1",
    "exportOptions": {
      "includeAnalytics": false,
      "includeLearningHistory": false,
      "includeProjectPatterns": false
    }
  }
}
