{
  "$schema": "claude-preferences-export-v1",
  "exportVersion": "1.0.0",
  "exportedAt": "2025-12-16T00:00:00Z",
  "exportedBy": "claude-config-template",
  "sourceVersion": "4.3.0",

  "exportType": "template",

  "templateMetadata": {
    "id": "gallery-devops",
    "name": "DevOps Infrastructure",
    "description": "Kubernetes, Terraform, and CI/CD workflows",
    "category": "devops",
    "recommendedFor": ["advanced", "expert"],
    "version": "1.0.0",
    "author": "claude-config-template",
    "extends": "team-standard",
    "_extendsHint": "Inherits all settings from team-standard, adds infrastructure-specific configuration"
  },

  "compatibility": {
    "minVersion": "4.3.0",
    "maxVersion": null
  },

  "contents": {
    "profile": {
      "primaryLanguages": ["yaml", "hcl", "bash", "python"],
      "primaryFrameworks": ["kubernetes", "terraform", "docker", "github-actions", "ansible"]
    },

    "confidenceThresholds": {
      "autoApply": 90,
      "suggestProminently": 70,
      "showAsOptional": 50,
      "hideBelow": 30
    },

    "learnedPreferences": {
      "workflow": {
        "infrastructure-as-code": true,
        "gitops": true,
        "immutable-infrastructure": true
      },
      "quality": {
        "dry-run-first": true,
        "rollback-plan": true,
        "monitoring-alerts": true
      }
    },

    "skillSpecificPreferences": {
      "security-scanner": {
        "checkSecrets": true,
        "checkIACVulnerabilities": true,
        "checkContainerImages": true,
        "checkNetworkPolicies": true,
        "checkRBAC": true
      },
      "test-generator": {
        "terraformValidation": true,
        "kubernetesLinting": true,
        "policyAsCode": true
      },
      "commit-readiness-checker": {
        "requirePlanOutput": true,
        "requireApproval": true
      }
    }
  },

  "metadata": {
    "checksum": "gallery-devops-v1",
    "exportOptions": {
      "includeAnalytics": false,
      "includeLearningHistory": false,
      "includeProjectPatterns": false
    }
  }
}
