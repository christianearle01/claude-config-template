  "sourceVersion": "4.0.0",

  "exportType": "template",

  "templateMetadata": {
    "id": "team-security",
    "name": "Team Security",
    "description": "Security-focused configuration for sensitive projects requiring extra caution",
    "category": "security",
    "recommendedFor": ["intermediate", "advanced", "expert"],
    "version": "1.0.0",
    "author": "your-team"
  },

  "compatibility": {
    "minVersion": "3.8.0",
    "maxVersion": "4.0.0"
  },

  "contents": {
    "profile": {
      "experienceLevel": "advanced",
      "proactivityLevel": "medium",
      "preferredWorkflow": "manual"
    },

    "confidenceThresholds": {
      "autoApply": 99,
      "suggestProminently": 90,
      "showAsOptional": 70,
      "hideBelow": 50
    },

    "learnedPreferences": {
      "workflow": {
        "preferred-commit-style": "conventional",
        "auto-stage-changes": false,
        "preferred-branch-naming": "feature/"
      },
      "quality": {
        "security-scan-threshold": "all",
        "test-coverage-target": 90,
        "lint-on-commit": true
      },
      "documentation": {
        "auto-update-changelog": true,
        "include-jsdoc": true
      }
    },

    "skillSpecificPreferences": {
      "security-scanner": {
        "showInfoLevel": true,
        "autoFixLowRisk": false,
        "ignoredPatterns": []
      },
      "test-generator": {
        "coverageReminders": true,
        "generateMocks": true
      },
      "commit-readiness-checker": {
        "requireChangelog": true,
        "requireTests": true,
        "conventionalCommits": true
      },
      "standards-enforcer": {
        "strictMode": true,
        "autoFormat": true,
        "customRules": []
      }
    }
  },

  "metadata": {
    "checksum": "template-team-security-v1",
    "exportOptions": {
